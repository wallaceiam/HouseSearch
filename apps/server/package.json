    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-security": "^1.4.0",
    "lint-staged": "^12.1.2",
    "node-mocks-http": "^1.11.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.5.1",
